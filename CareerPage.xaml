<Window x:Class="OWLSimGame.CareerPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OWLSimGame"
        mc:Ignorable="d"
        Title="CareerPage" Height="350" Width="525" WindowState="Maximized" WindowStyle="None">
    <Grid>
        <Button x:Name="closeGame" Content="Close Game" HorizontalAlignment="Center" Height="90" VerticalAlignment="Top" Width="200" Click="closeWindow" Margin="0,597,0,0"/>
        <Button Content="Play Game" HorizontalAlignment="Center" Height="90" Margin="0,413,0,0" VerticalAlignment="Top" Width="200" Click="Button_Click"/>
        <TextBox x:Name="tbxMessageTest" HorizontalAlignment="Center" Height="60" Margin="0,508,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="200" TextChanged="TextBox_TextChanged"/>

    </Grid>
</Window>
